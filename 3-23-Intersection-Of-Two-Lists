# A = [4,2,1,3]
# B = [5,6,4,3]


#[4,3]

def intersection(list1, list2):
    hashSet = set(list1)
    result = []
    
    for val in list2:
        if val in hashSet:
            result.append(val)
    
    return result

A = [4,2,1,3]
B = [5,6,4,3]
result = intersection(A, B)
print(result)

A = [2,5]
B = [2,5]
result = intersection(A, B)
print(result)

A = [2,5,4, 8]
B = []
result = intersection(A, B)
print(result)
