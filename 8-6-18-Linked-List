public class Node<T> {
    private T value;
    private Node next;

    public Node(T val) {
        value = val;
        next = null;
    }

    public Node(T val, Node n) {
        value = val;
        next = n;
    }

    public T getValue() {
        return value;
    }

    public Node getNext() {
        return next;
    }

    public void setValue(T val) {
        value = val;
    }

    public void setNext(Node n) {
        next = n;
    }


}
